<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="compactDisc" class="WiringBeans.SoundSystem.SgtPeppers"></bean>
    <bean id="cdPlayer" class="WiringBeans.SoundSystem.BlankDisc">
        <constructor-arg value="Sgt.Pepper's Lonely Hearts Club Band"></constructor-arg>
        <constructor-arg value="The Beatles"></constructor-arg>
        <constructor-arg>
            <list>
                <value>Sgt.Pepper's Lonely Hearts Club Band</value>
                <value>With a Little Help From My Friends</value>
                <value>Luck in the Sky with Diamonds</value>
            </list>
        </constructor-arg>
    </bean>
</beans>